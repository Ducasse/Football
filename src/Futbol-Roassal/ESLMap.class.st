{ #category : #hooks }
ESLMap class >> europe [
	^ #('Germany' 'France' 'Italy' 'Spain' 'Poland' 'Romania' 'Netherlands' 'Belgium' 'Greece' 'Portugal' 'Sweden' 'Austria' 'Serbia' 'Switzerland' 'England' 'Luxembourg' 'Denmark' 'Norway' 'Croatia' 'CzechRepublic' 'Slovakia' 'Hungary' 'Ireland' 'BosniaandHerzegovina' 'Albania' 'RepublicofMacedonia' 'Scotland')
]

{ #category : #'accessing - defaults' }
ESLMap >> defaultContainer [
	^ RSCanvas new @ RSCanvasController
]

{ #category : #hooks }
ESLMap >> europeShape [
	^ (self class europe collect: [ :name | | league path |
		path := RSSVGPath new.
		path border: (RSBorder new color: Color black; width: 0.1).
		league := self leagueByCountryName: name.
		
		league ifNotNil: [ 
			
			path color: (Color blue alpha: 0.2).
			path popup.
			path model: league ].
		path
			svgPath: (name asSymbol value: self class);
			yourself ]) asShape.
]

{ #category : #hooks }
ESLMap >> leagueByCountryName: name [
	^ self leagues detect: [ :league | league countryName = name ] ifNone: [ nil ]
]

{ #category : #accessing }
ESLMap >> leagues [
	^ leagues
]

{ #category : #accessing }
ESLMap >> leagues: aCollection [ 
	leagues := aCollection
]

{ #category : #hooks }
ESLMap >> renderIn: aCanvas [
	aCanvas add: self europeShape.
	
	
]
